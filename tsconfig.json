{
  "compilerOptions": {
    "target": "ES2020",
    "module": "esnext",
    "outDir": "dist",
    "rootDir": "src",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": false,
    "skipLibCheck": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "src/routes/common_backup",
    "**/*.disabled.ts",
    "**/*.disabled.vue"
  ],
}